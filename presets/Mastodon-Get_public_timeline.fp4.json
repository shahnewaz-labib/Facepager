{
  "name": "Get public timeline",
  "category": "Mastodon",
  "description": "This is a simple preset that serves as a starting point to dive into the mastodon fediverse.\n\nFor retrieving data from Mastodon, you first have to decide for an instance such as sciences.social or mastodon.social. Adjust the base path accordingly.\n\nTo get the public timeline, add \"public\" as a seed not and fetch the data.\n\nFor further information, see https://docs.joinmastodon.org/client/public/",
  "module": "Generic",
  "options": {
    "basepath": "https://sciences.social/api/v1",
    "resource": "/timelines/<Object ID>",
    "params": {},
    "extension": "",
    "headers": {},
    "verb": "GET",
    "format": "json",
    "filename": "<None>",
    "fileext": "<None>",
    "pages": 1,
    "paging_type": "key",
    "key_paging": null,
    "paging_stop": null,
    "param_paging": null,
    "nodedata": null,
    "objectid": "id",
    "scope": "identity read mysubreddits",
    "proxy": "",
    "auth_type": "Disable",
    "auth_uri": "https://www.reddit.com/api/v1/authorize?state=mystate&duration=temporary",
    "redirect_uri": "https://localhost",
    "token_uri": "https://www.reddit.com/api/v1/access_token",
    "auth": "disable",
    "auth_tokenname": "",
    "auth_prefix": ""
  },
  "speed": 200,
  "saveheaders": false,
  "timeout": 15,
  "maxsize": 5,
  "columns": [
    "created_at",
    "account.acct",
    "content",
    "replies_count",
    "reblogs_count",
    "favourites_count",
    "uri"
  ]
}